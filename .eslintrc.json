{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console":0,
        "no-param-reassign":0,
        "no-use-before-define":0,
        "import/extensions":0,
        "import/newline-after-import":0,
        "import/no-unresolved":0,
        "no-inner-declarations":0
        
    }
}
